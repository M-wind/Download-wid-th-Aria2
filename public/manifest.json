{
  "name": "Download with Aria2",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["downloads", "storage", "cookies"],
  "action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png"
    },
    "default_popup": "index.html"
  },
  "default_locale": "zh",
  "description": "__MSG_description__",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"]
}
